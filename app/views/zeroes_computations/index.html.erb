<p id="notice"><%= notice %></p>

<h1>Zeroes Computations</h1>

<table>
  <thead>
    <tr>
      <th>Expression</th>
      <th>Result</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @zeroes_computations.each do |zeroes_computation| %>
      <tr>
        <td><%= zeroes_computation.expression %></td>
        <td><%= zeroes_computation.result %></td>
        <td><%= link_to 'Show', zeroes_computation %></td>
        <td><%= link_to 'Edit', edit_zeroes_computation_path(zeroes_computation) %></td>
        <td><%= link_to 'Destroy', zeroes_computation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Zeroes Computation', new_zeroes_computation_path %>
